	${EGO_SUM_SRC_URI}"

LICENSE="Apache-2.0"
SLOT="0"
KEYWORDS="~amd64"
IUSE="doc"

S="${WORKDIR}/relay-${PV}"

DEPEND=">=dev-lang/go-1.13"
RDEPEND="${DEPEND}"
BDEPEND=""

src_unpack() {
	go-module_src_unpack
}

src_compile() {
	go build -o relay ./cmd/relay
}

src_install() {
	dobin relay
	if use doc ; then
		dodoc README.md
	fi
}
